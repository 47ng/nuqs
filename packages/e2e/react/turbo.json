{
  "$schema": "https://turbo.build/schema.v2.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "FULL_PAGE_NAV_ON_SHALLOW_FALSE",
        "REACT_COMPILER",
        "E2E_NO_CACHE_ON_RERUN"
      ]
    },
    "test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": [
        "FULL_PAGE_NAV_ON_SHALLOW_FALSE",
        "REACT_COMPILER",
        "E2E_NO_CACHE_ON_RERUN"
      ]
    }
  }
}
