{
  "$schema": "https://turbo.build/schema.v2.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    }
  }
}
