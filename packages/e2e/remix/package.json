{
  "name": "e2e-remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev --port 3003",
    "start": "cross-env NODE_ENV=production PORT=3003 remix-serve ./build/server/index.js",
    "pretest": "playwright install chromium",
    "test": "pnpm run --stream '/^test:/'",
    "test:types": "tsc",
    "test:playwright": "playwright test --project=chromium"
  },
  "dependencies": {
    "@remix-run/node": "^2.17.3",
    "@remix-run/react": "^2.17.3",
    "@remix-run/serve": "^2.17.3",
    "isbot": "^5.1.32",
    "nuqs": "workspace:*",
    "react": "catalog:react19",
    "react-dom": "catalog:react19"
  },
  "devDependencies": {
    "@playwright/test": "catalog:e2e",
    "@remix-run/dev": "^2.17.3",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "cross-env": "^10.0.0",
    "e2e-shared": "workspace:*",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
