{
  "name": "e2e-react-router-v7",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev --port 3007",
    "start": "cross-env NODE_ENV=production ./server.mjs",
    "pretest": "cypress install && playwright install chromium",
    "test": "pnpm run test:types && pnpm run test:playwright && pnpm run test:cypress",
    "test:types": "react-router typegen && tsc",
    "test:playwright": "playwright test --project=chromium",
    "test:cypress": "start-server-and-test start http://localhost:3007 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cross-env FORCE_COLOR=3 cypress run --headless"
  },
