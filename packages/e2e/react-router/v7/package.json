    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "compression": "^1.8.1",
    "cross-env": "^10.0.0",
    "cypress": "catalog:e2e",
    "e2e-shared": "workspace:*",
    "express": "^4.22.0",
