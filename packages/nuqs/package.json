    "typescript": "^5.9.2",
    "valibot": "^1.1.0",
    "vitest": "^3.2.4",
    "vitest-package-exports": "^0.1.1",
    "zod": "^4.1.5"
  },
  "size-limit": [
    {
      "name": "Client",
      "path": "dist/index.js",
      "limit": "6.01 kB"
    },
    {
      "name": "Client (minimal tree-shaken)",
      "path": "dist/index.js",
      "limit": "4.5 kB",
      "import": "{ useQueryStates, parseAsInteger }"
    },
    {
      "name": "Server",
      "path": "dist/server.js",
      "limit": "3.5 kB"
    }
  ]
