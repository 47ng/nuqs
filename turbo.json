{
  "$schema": "https://turbo.build/schema.v2.json",
  "concurrency": "11",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "nuqs#build": {
      "outputs": ["dist/**", ".tsup/**", ".tsbuildinfo"]
    },
    "nuqs#build:size-json": {
      "outputs": ["size.json"],
      "dependsOn": ["build"]
    },
    "e2e-next#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"],
      "env": ["BASE_PATH", "REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-remix#build": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v5#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v6#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v6-hash#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v7#build": {
      "outputs": [".react-router/**", "build/**"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react#build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"],
      "env": [
        "FULL_PAGE_NAV_ON_SHALLOW_FALSE",
        "REACT_COMPILER",
        "E2E_NO_CACHE_ON_RERUN"
      ]
    },
    "e2e-tanstack-router#build": {
      "outputs": ["dist/**", "src/routeTree.gen.ts"],
      "dependsOn": ["^build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "docs#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build", "nuqs#build:size-json"],
      "env": [
        "VERCEL_ENV",
        "VERCEL_URL",
        "VERCEL_PROJECT_PRODUCTION_URL",
        "ENABLE_SENTRY",
        "SENTRY_ORG",
        "SENTRY_PROJECT",
        "SENTRY_AUTH_TOKEN",
        "NEXT_PUBLIC_SENTRY_DSN",
        "GITHUB_TOKEN",
        "ISR_TOKEN"
      ]
    },
    "nuqs#test": {
      "outputs": ["dist/**", "coverage/**"],
      "dependsOn": ["build"]
    },
    "docs#test": {
      "dependsOn": ["^build"]
    },
    "e2e-next#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["BASE_PATH", "REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-remix#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v5#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v6#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v6-hash#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react-router-v7#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    },
    "e2e-react#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": [
        "FULL_PAGE_NAV_ON_SHALLOW_FALSE",
        "REACT_COMPILER",
        "E2E_NO_CACHE_ON_RERUN"
      ]
    },
    "e2e-tanstack-router#test": {
      "outputs": [".playwright/**"],
      "dependsOn": ["build"],
      "env": ["REACT_COMPILER", "E2E_NO_CACHE_ON_RERUN"]
    }
  }
}
