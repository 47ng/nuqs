{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "next-usequerystate#build": {
      "outputs": ["dist/**"]
    },
    "e2e#build": {
      "outputs": [".next/**", "!.next/cache/**", "cypress/**"],
      "dependsOn": ["^build"],
      "env": ["BASE_PATH", "WINDOW_HISTORY_SUPPORT"]
    },
    "docs#build": {
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "next-usequerystate#test": {
      "outputs": ["dist/**", "coverage/**"],
      "dependsOn": ["build"]
    },
    "e2e#test": {
      "dependsOn": ["build"]
    }
  }
}
